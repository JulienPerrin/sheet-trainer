    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/router": "^11.0.9",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@types/webmidi": "^2.0.4",
    "beautiful-piano": "0.0.6",
    "bootstrap": "^4.6.0",
